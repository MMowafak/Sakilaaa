1. select * from Actors;
2. select first_name from actor where first_name = 'JOHN';
3. select * from actor where last_name = 'NEESON';
4. select * from actor where (actor_id%10) = 0;
5. select * from film where film_id = 100;
6. select * from film where rating = 'R';
7. select * from film_list where rating = 'R';
8. select * from film order by length limit 10;
9. select title  from film order by length limit 10;
10. select * from film where special_features = 'Deleted Scenes';
11. select distinct last_name from actor;
12. select last_name, count(*) from actor group by last_name having count(*) > 1 ;
13. select actors,  count(actors) from film_list group by actors having count(actors) > 1 ;
14. select count(film_id) from inventory where film_id = 1;
15. select last_update from inventory where film_id = 1;
